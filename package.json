{
  "name": "addtopage",
  "version": "0.1.0",
  "description": "add content to your page - how you want - using JavaScript",
  "main": "dist/addtopage.bundle.js",
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack -p",
    "start": "webpack -p --watch",
    "test": "nyc ava"
  },
  "author": "Blake Knight <bknight@gsandf.com> (http://blakek.me/)",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.12.0",
    "express": "^4.14.0",
    "jsdom": "^9.2.1",
    "nyc": "^6.6.1",
    "webpack": "^1.13.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test/*.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "dependencies": {
    "axios": "^0.12.0"
  }
}
